@model UltimateWeb.Models.Usuario

@{
    ViewData["Title"] = "Cadastro de usuario";
}


<br />
<br />
<br />
<h4>Dados do usuario</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreateUser">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group col-sm-9 col-lg-12">
                <label asp-for="Username" class="control-label"></label>
                <input asp-for="Username" class="form-control" />

            </div>
            <div class="form-group col-sm-9 col-lg-12">
                <label asp-for="Senha" class="control-label"></label>
                <input asp-for="Senha" class="form-control" />

            </div>
            <div class="form-group col-sm-9 col-lg-12">
                <label asp-for="Email" class="control-label"></label>
                <input asp-for="Email" class="form-control" />

            </div>
            <div class="form-group col-sm-9 col-lg-12">
                <label asp-for="Permissoes" class="control-label"></label>
                <input asp-for="Permissoes" class="form-control" />
            </div>
            
           
               <button value="SALVAR" class="btn btn-primary" />
          
            
        </form>
